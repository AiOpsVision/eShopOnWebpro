{
  "AllowedHosts": "*",
  "baseUrls": {
    "apiBase": "https://localhost:5099/api/",
    "webBase": "https://localhost:44315/"
  },
  "CatalogBaseUrl": "",
  "ConnectionStrings": {
    "CatalogConnection": "Server=sqlserver,${DB_PORT};Database=${DB_CATALOG};User Id=${DB_USER};Password=${DB_PASS};Encrypt=False;TrustServerCertificate=True",
    "IdentityConnection": "Server=sqlserver,${DB_PORT};Database=${DB_IDENTITY};User Id=${DB_USER};Password=${DB_PASS};Encrypt=False;TrustServerCertificate=True"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  }
}



